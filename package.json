{
  "name": "my-node-bootstrap",
  "version": "0.1.0",
  "description": "My Node-bootstrap with Typescript + Express",
  "main": "dist/index.js",
  "scripts": {
    "dev": "pm2 start pm2-dev-app.yml && pm2 logs my-node-bootstrap",
    "db:add": "sh ./db/scripts/add-test-data.sh",
    "db:connect": "./db/scripts/connect-to-db.sh",
    "db:docker": "docker exec -it db_my_postgres_1 bash",
    "db:migrate": "flyway -c db/migrations/conf.js migrate",
    "db:reset": "sh ./db/scripts/reset-db.sh",
    "db:seed": "flyway -c db/seed/conf.js migrate",
    "db:start": "docker-compose -f ./db/docker-compose.yml up -d",
    "db:stop": "docker-compose -f ./db/docker-compose.yml down",
    "lint": "tslint --project ./ -c tslint.json",
    "test": "jest",
    "ts:watch": "tsc -w",
    "ts": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeemuKoivisto/my-node-bootstrap.git"
  },
  "keywords": [
    "Node",
    "TypeScript",
    "Express.js",
    "PostgreSQL",
    "AWS"
  ],
  "author": "Teemu Koivisto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeemuKoivisto/my-node-bootstrap/issues"
  },
  "homepage": "https://github.com/TeemuKoivisto/my-node-bootstrap#readme",
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/joi": "13.4.2",
    "@types/jsonwebtoken": "7.2.8",
    "@types/morgan": "^1.7.35",
    "@types/pg": "^7.4.10",
    "@types/winston": "^2.3.9",
    "dotenv": "^6.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "joi": "13.6.0",
    "jsonwebtoken": "8.3.0",
    "morgan": "^1.9.0",
    "node-flywaydb": "^0.1.4",
    "pg": "^7.4.3",
    "pm2": "^3.0.3",
    "winston": "^3.0.0"
  }
}
